{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "single"],
        "indent": ["error", 2],
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "no-undef":"off",
        "no-underscore-dangle":"off"
    }
}
